<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in pages %}
        <url>
            <loc>http://www.chrissheppard.co.uk{{ page.url }}</loc>
            {#<lastmod>moment(page.updated).format("YYYY-MM-DD")</lastmod>#}
            <changefreq>monthly</changefreq>
            {#<priority>0.8</priority>#}
        </url>
    {% endfor %}
    {% for post in posts %}
        <url>
            <loc>http://www.chrissheppard.co.uk{{ post.url }}</loc>
            {#<lastmod>moment(post.updated).format("YYYY-MM-DD")</lastmod>#}
            <changefreq>monthly</changefreq>
            {#<priority>0.8</priority>#}
        </url>
    {% endfor %}
</urlset>